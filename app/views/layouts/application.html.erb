<% content_for :top_nav do %>
  <%= render 'shared/top_nav' %>
<% end %>

<%= yield %>

<% content_for :javascripts do %>
  <%= javascript_include_tag "application" %>
  <%= yield :js %>
<% end %>

<% content_for :stylesheets do %>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= yield :css %>
<% end %>

<%= parent_layout '_bootstrap' %>
